package ngoding ;
import java.lang.StringBuilder;
public class day89 {
    public static void main(String[]args){
       StringBuilder SB = new StringBuilder("hai"); 
        System.out.println("base = "+SB);
        System.out.println("kapasitas = "+SB.capacity());
        
        
        int addres = System.identityHashCode(SB);
        System.out.print("address = "+ Integer.toHexString(addres));
    }
}
