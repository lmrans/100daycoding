package ngoding ;

public class day74 {
    public static void main(String[]args){
        
        int tinggi = 5; 
        int baris = 1;
        while (baris <= tinggi) {
            int kolom = 1;
            while (kolom <= baris) {
                System.out.print("* ");
                kolom++;
            }
            System.out.println();
            baris++;
        }
    }
    
}
