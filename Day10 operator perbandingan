package ngoding ;

public class java {
    public static void main(String[]args){
        
    int a = 7;
        int b = 6;
       // operator perbandingan
        
        System.out.println(b==a);
        System.out.println(a!=b);
        System.out.println(a>b);
        System.out.println(a<b);
        System.out.println(a<=b);
        System.out.println(a>=b);

   }
}
