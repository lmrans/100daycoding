package ngoding ;
import java.util.Scanner;
public class ke14 {
    public static void main(String[]args){
        Scanner input = new Scanner(System.in);
        System.out.print("masukkan nilai = ");
        int c = input.nextInt();
        for(int a = c; c <=10; c+=2){
          System.out.println(c);  
        }
        
    }
}
