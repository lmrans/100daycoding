package ngoding ;
import java.util.Scanner;
public class day62 {
    public static void main(String[]args){
        Scanner input = new Scanner(System.in);
        System.out.print("Masukka nilai : ");
        int nilai = input.nextInt();
        boolean periksa;
        for(int i = 2; i < nilai; i++){
            periksa = true;
            for(int a = 2; a <= (i/2); a++){
                if(i % a==0){
                    periksa = false;
                    break;
                }
            }
            if(periksa)
                System.out.println("bilangan prima : "+i);
        }
    }
}
