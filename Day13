package ngoding ;

public class ke13 {
    public static void main(String[]args){
        int angka = 4;
        angka /= 20;
        angka *= 5;
        angka += 6;
        angka -= 16;
        System.out.println(angka);
        
        int nilai = 5;
        int skor = 8;
        System.out.println(nilai != skor);
        System.out.println(nilai > skor);
        System.out.println(nilai < skor);
        System.out.println(nilai == skor);
        System.out.println(nilai >= skor);
        System.out.println(nilai <= skor);
        
        System.out.println(nilai > skor || nilai < skor);
        System.out.println(nilai > skor || nilai == skor);
        
        
        
        
    }     
    
}
