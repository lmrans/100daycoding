public class Main {
    
    public static void main(String[] args) {
        long a = 20 , c = 15 , v;
        
        v = a + c;
        System.out.println("= "+v);
        v = a - c;
        System.out.println("= "+v);
        v = a * c;
        System.out.println("= "+v);
        v = a / c;
        System.out.println("= "+v);
        v = a % c;
        System.out.println("= "+v);
        
    }
    
}
