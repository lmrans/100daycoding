package ngoding ;

public class day36 {
    public static void main(String[]args){
        int agk = 4;
        for(int a = 1; a <= agk; a++){
            for(int b = 1; b <= agk; b++){
                System.out.print("*");
            }
            System.out.println("*");
        }
        
    }
    
}
