package ngoding ;

public class da49 {
    public static void main(String[]args){
        int [] a = {1,2,3,40,5,6};
        int jml = a.length;
        int max= a[0],min=a[0];
        for(int i = 0; i< jml; i++){
            if(a[i]>max){
                max = a[i];
            }
            if(a[i]<min){
                min = a[i];
            }
        }
        System.out.println("nilai max dalam array = "+max);
        System.out.println("nilai min dalam array = "+min);
    }
    
}
